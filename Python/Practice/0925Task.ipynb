    "def solution(data):\n",
    "    sandwich = []\n",
    "    cnt = 0\n",
    "\n",
    "    for i in data:\n",
    "        sandwich.append(i)\n",
    "        if sandwich[-5:] == [1, 2, 3, 4, 1]:\n",
    "            del sandwich[-5:]\n",
    "            cnt += 1\n",
    "    return cnt\n",
    "print(solution([1, 2, 3, 4, 1]))\n",
    "print(solution([1, 2, 3, 4, 1, 2, 3, 4, 1]))"
   ]
